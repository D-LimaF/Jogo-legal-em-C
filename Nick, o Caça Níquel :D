#include <stdio.h>
#include <time.h>


int main(){

int v1[1], v2[1], v3[1];

printf("Seja bem-vindo, eu sou Nick, seu furtador virtual. \n Vamos jogar? \n");

srand(time(NULL)); //Vai gerar um número aleatório 

for(int i = 0; i<1; i++){

v1[i] = rand() %9; 
printf ("%d\n",v1[i]);

for(int x = 0; x<1; x++){

v2[x] = rand() %9; 
printf ("%d\n",v2[x]);

for(int y = 0; y<1; y++){

v3[y] = rand() %9; 
printf ("%d\n",v3[y]);

if((v1[i]==v2[x]) && (v2[x]==v3[y]) && v1[i]==v3[y]){

    puts("+3 credits!!");}
    
    if((v1[i]!=v2[x]) && (v2[x]==v3[y]) && v1[i]==v3[y]){

    puts("+1 credits!!");}
    
    if((v1[i]==v2[x]) && (v2[x]!=v3[y])){

    puts("+1 credits!!");}

    if((v1[i]!=v2[x]) && v1[i]==v3[y]){

    puts("+1 credits!!");}

    if((v2[x]==v3[y]) && v1[i]!=v3[y]){

    puts("+1 credits!!");}

}}}


}

